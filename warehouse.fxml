			<font>
				<Font name="Times New Roman Bold Italic" size="17.0" />
			</font>
		</Button>
		<Button fx:id="deleteStoreBtn" layoutX="567.0" layoutY="274.0"
			mnemonicParsing="false" onAction="#delete"
			style="-fx-background-color: ORANGE;" text="Delete">
			<font>
				<Font name="Times New Roman Bold Italic" size="16.0" />
			</font>
		</Button>
		<Button fx:id="backToStockedBtn" layoutX="14.0" layoutY="473.0"
			mnemonicParsing="false" onAction="#stock"
			style="-fx-background-color: orange;"
			text="Back to the stockedProduct">
			<font>
				<Font name="Times New Roman Bold Italic" size="15.0" />
			</font>
		</Button>
		<Button fx:id="mainBtn" layoutX="26.0" layoutY="22.0"
			mnemonicParsing="false" onAction="#main" prefHeight="25.0"
			prefWidth="117.0" style="-fx-background-color: orange;"
			text="Back to the main">
			<font>
				<Font name="Times New Roman Bold Italic" size="14.0" />
			</font>
		</Button>
