<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="storepane" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="525.0" prefWidth="650.0"
	style="-fx-background-color: BISQUE;"
	xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.WarehouseController">
	<children>
		<Label fx:id="storelbl" layoutX="197.0" layoutY="53.0"
			prefHeight="17.0" prefWidth="255.0" text="warehouse">
			<font>
				<Font name="Times New Roman Bold Italic" size="50.0" />
			</font>
		</Label>
		<TableView fx:id="storetv" layoutX="26.0" layoutY="124.0"
			prefHeight="235.0" prefWidth="317.0">
			<columns>
				<TableColumn fx:id="nameCoulmn"
					prefWidth="121.33333778381348" text="Name" />
				<TableColumn fx:id="addressCoulmn"
					prefWidth="194.6666717529297" text="Address" />
			</columns>
		</TableView>
		<Label fx:id="storeNameLbl" layoutX="456.0" layoutY="124.0"
			prefHeight="17.0" prefWidth="117.0" text="Warehouse Name">
			<font>
				<Font name="Times New Roman Bold Italic" size="15.0" />
			</font>
		</Label>
		<Label fx:id="storeAddressLbl" layoutX="448.0" layoutY="205.0"
			prefHeight="17.0" prefWidth="128.0" text="Warehouse Address">
			<font>
				<Font name="Times New Roman Bold Italic" size="15.0" />
			</font>
		</Label>
		<TextField fx:id="storeNameTxt" layoutX="453.0"
			layoutY="151.0" />
		<TextField fx:id="storeAddressTxt" layoutX="448.0"
			layoutY="229.0" />
		<Button fx:id="insertStoreBtn" layoutX="364.0" layoutY="274.0"
